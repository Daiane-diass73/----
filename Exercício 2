  return $this -> salarioMensal = $recebe2;
  }
  
  public function getSalarioMensal(){
  return $this -> salarioMensal;
  }
  
  função publica setSalarioMensal(){
  retornar $this -> salarioMensal = $this -> salarioMensal * 12;
  }
  
    função pública getSalarioMensal () {
    retornar $ this -> salarioMensal;
  }
  função pública validaSalarioMensal () {
    if ($ this-> salarioMensal <0) {
      $ this-> salarioMensal = 0,0;
    }
  }
  public function aumentasalarioMensal () {
    $ porcen = ($ this-> salarioMensal * 10) / 100;
    $ this-> salarioMensal = $ this-> salarioMensal + $ porcen;
  }
 
 // Empregado 1


$ empregado1 = novos Empregados ();
